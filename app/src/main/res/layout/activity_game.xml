<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/ace"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="8dp"
        android:focusable="auto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/a_s" />

    <ImageView
        android:id="@+id/two"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ace"
        app:srcCompat="@drawable/two_s" />

    <ImageView
        android:id="@+id/three"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/two"
        app:srcCompat="@drawable/three_s" />

    <ImageView
        android:id="@+id/four"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/three"
        app:srcCompat="@drawable/four_s" />

    <ImageView
        android:id="@+id/five"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/four"
        app:srcCompat="@drawable/five_s" />

    <ImageView
        android:id="@+id/six"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/ace"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/six_s" />

    <ImageView
        android:id="@+id/seven"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/two"
        app:layout_constraintStart_toEndOf="@+id/six"
        app:srcCompat="@drawable/seven_s" />

    <ImageView
        android:id="@+id/eight"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/three"
        app:layout_constraintStart_toEndOf="@+id/seven"
        app:srcCompat="@drawable/eight_s" />

    <ImageView
        android:id="@+id/nine"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/four"
        app:layout_constraintStart_toEndOf="@+id/eight"
        app:srcCompat="@drawable/nine_s" />

    <ImageView
        android:id="@+id/ten"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/five"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/nine"
        app:srcCompat="@drawable/ten_s" />

    <ImageView
        android:id="@+id/jack"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/seven"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/jack_s" />

    <ImageView
        android:id="@+id/king"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="91dp"
        android:layout_marginRight="91dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/nine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/queen"
        app:srcCompat="@drawable/king_s" />

    <ImageView
        android:id="@+id/queen"
        android:layout_width="70dp"
        android:layout_height="105dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/eight"
        app:layout_constraintStart_toEndOf="@+id/jack"
        app:srcCompat="@drawable/queen_s" />

    <ImageView
        android:id="@+id/deckCards"
        android:layout_width="90dp"
        android:layout_height="115dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="70dp"
        app:layout_constraintBottom_toTopOf="@+id/queenNumber"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/deck_cards" />

    <TextView
        android:id="@+id/aceNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/ace"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/six" />

    <TextView
        android:id="@+id/threeNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/three"
        app:layout_constraintStart_toEndOf="@+id/twoNumber"
        app:layout_constraintTop_toBottomOf="@+id/eight" />

    <TextView
        android:id="@+id/jackNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/jack"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/twoNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/two"
        app:layout_constraintStart_toEndOf="@+id/aceNumber"
        app:layout_constraintTop_toBottomOf="@+id/seven" />

    <TextView
        android:id="@+id/fiveNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/five"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fourNumber"
        app:layout_constraintTop_toBottomOf="@+id/ten" />

    <TextView
        android:id="@+id/fourNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/four"
        app:layout_constraintStart_toEndOf="@+id/threeNumber"
        app:layout_constraintTop_toBottomOf="@+id/nine" />

    <TextView
        android:id="@+id/queenNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/queen"
        app:layout_constraintStart_toEndOf="@+id/jackNumber" />

    <TextView
        android:id="@+id/kingNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="91dp"
        android:layout_marginRight="91dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/king"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/queenNumber" />

    <TextView
        android:id="@+id/sixNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="2dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/six"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/jack"
        app:layout_constraintVertical_bias="0.636" />

    <TextView
        android:id="@+id/sevenNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="2dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/six"
        app:layout_constraintStart_toEndOf="@+id/sixNumber"
        app:layout_constraintTop_toBottomOf="@+id/jack"
        app:layout_constraintVertical_bias="0.636" />

    <TextView
        android:id="@+id/eightNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="2dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/six"
        app:layout_constraintStart_toEndOf="@+id/sevenNumber"
        app:layout_constraintTop_toBottomOf="@+id/jack"
        app:layout_constraintVertical_bias="0.636" />

    <TextView
        android:id="@+id/nineNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="2dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/six"
        app:layout_constraintStart_toEndOf="@+id/eightNumber"
        app:layout_constraintTop_toBottomOf="@+id/jack"
        app:layout_constraintVertical_bias="0.636" />

    <TextView
        android:id="@+id/tenNumber"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginBottom="2dp"
        android:text="@string/number_of_cards"
        android:textColor="@color/textColor"
        app:layout_constraintBottom_toTopOf="@+id/six"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/nineNumber"
        app:layout_constraintTop_toBottomOf="@+id/jack"
        app:layout_constraintVertical_bias="0.636" />

</androidx.constraintlayout.widget.ConstraintLayout>